class Solution {
    public int strStr(String haystack, String needle) {
        int str1=0;
        int str2=0;
        str1=haystack.length();
        str2=needle.length();
        int index=0;
        int t=0;
        if(str1>=str2){
        for(int i=0;i<str1;i++){
            if(haystack.charAt(i)==needle.charAt(t)){
                t++;
                if(str2==t){
                    index=i;
                    break;
                }
            }
            else{
                i=i-t;
                t=0;
            }
        }
        if(str2!=t)
            t=0;
            if(t==0){
                return -1;
        }
        else{
            return (index-t)+1;    
        }
        }
        else{
            return -1;
        }
    }
}
